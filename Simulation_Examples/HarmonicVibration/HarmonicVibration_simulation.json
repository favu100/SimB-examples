{
    "activations": [
        {
            "id": "$setup_constants",
            "execute": "$setup_constants",
            "activating": "$initialise_machine"
        },
        {
            "id": "$initialise_machine",
            "execute": "$initialise_machine",
            "activating": "move"
        },
        {
            "id": "move",
            "execute": "move",
            "after": "ROUND(RINTEGER(timeStep * 1000.0))",
            "activating": "move"
        }
    ]
}
